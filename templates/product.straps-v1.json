      "blocks": {
        "template--20972192301366__d625e37a-d058-4361-9b4e-64f1973a8bbc-article-2": {
          "type": "article",
          "settings": {
            "title": "Specs",
            "text": "<ul><li><strong>Sold in pairs.</strong></li><li><strong>Length</strong>: 13\"</li><li><strong>Width</strong>: 2.5\"</li><li><strong>Thickness</strong>: 1/16\"</li><li><strong>Material</strong>: cotton</li></ul>"
